{
  "name": "@rhao/request-basic-middleware",
  "type": "module",
  "version": "0.0.0",
  "description": "Basic middleware of request.",
  "author": "RayHao <https://github.com/l246804>",
  "license": "MIT",
  "homepage": "https://github.com/l246804/request/tree/dev/packages/basic-middleware",
  "repository": {
    "type": "git",
    "url": "https://github.com/l246804/request"
  },
  "bugs": {
    "url": "https://github.com/l246804/request/issues"
  },
  "keywords": [
    "middleware",
    "request",
    "basic",
    "common"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*",
    "./debounce": {
      "types": "./dist/debounce.d.ts",
      "require": "./dist/debounce.cjs",
      "import": "./dist/debounce.mjs"
    },
    "./throttle": {
      "types": "./dist/throttle.d.ts",
      "require": "./dist/throttle.cjs",
      "import": "./dist/throttle.mjs"
    },
    "./refresh": {
      "types": "./dist/refresh.d.ts",
      "require": "./dist/refresh.cjs",
      "import": "./dist/refresh.mjs"
    },
    "./retry": {
      "types": "./dist/retry.d.ts",
      "require": "./dist/retry.cjs",
      "import": "./dist/retry.mjs"
    },
    "./swr": {
      "types": "./dist/swr.d.ts",
      "require": "./dist/swr.cjs",
      "import": "./dist/swr.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild"
  },
  "dependencies": {
    "@rhao/request": "workspace:*",
    "@rhao/request-utils": "workspace:*",
    "@types/lodash-es": "^4.17.8",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "types": "workspace:*",
    "unbuild": "^1.2.1"
  }
}
