{
  "name": "@rhao/request",
  "type": "module",
  "version": "3.0.1",
  "description": "Data request state management library.",
  "author": "RayHao <https://github.com/l246804>",
  "license": "MIT",
  "homepage": "https://github.com/l246804/request",
  "repository": {
    "type": "git",
    "url": "https://github.com/l246804/request"
  },
  "bugs": {
    "url": "https://github.com/l246804/request/issues"
  },
  "keywords": [
    "request",
    "fetch",
    "axios",
    "state",
    "vue",
    "react",
    "hooks",
    "compose",
    "event",
    "middleware"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./builtin-middleware/*": {
      "types": "./dist/builtin-middleware/*.d.ts",
      "require": "./dist/builtin-middleware/*.cjs",
      "import": "./dist/builtin-middleware/*.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild"
  },
  "dependencies": {
    "@rhao/lodash-x": "^0.4.0",
    "@rhao/types-base": "^0.2.0",
    "@types/lodash": "^4.14.197",
    "@types/lodash-es": "^4.17.8",
    "hookable": "^5.5.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3"
  },
  "devDependencies": {
    "unbuild": "^1.2.1"
  }
}
